<?xml version="1.0"?>
<test name="interface up. Full. Positive" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>PPPoE5</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface pppoe service">
        <name>PPPoE5</name>
      </command>
        <assert type="tag" name="message" value="Interface enabled." />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
      <command name="interface">
        <name>PPPoE5</name>
      </command>
    </request>
  </teardown>
</test>
