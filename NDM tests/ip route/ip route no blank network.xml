<?xml version="1.0"?>
<test name="ip route. Blank network. Negative" groups="mobile">
  <body>
    <packet>
      <request>
      <command name="ip route">
        <default>1</default>
        <host></host>
        <gateway></gateway>
        <no>no</no>
        <mask></mask>
        <interface>ISP</interface>
        <network></network>
      </command>
        <assert type="error" />
      <assert type="xpath" path="//error[@source='network']" />
      </request>
    </packet>
  </body>
</test>
