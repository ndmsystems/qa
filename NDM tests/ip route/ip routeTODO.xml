<?xml version="1.0"?>
<test name="ip route. Full. Positive" groups="mobile">
  <setup>
    <request><command name="ip route">
    <gateway>0.0.0.0</gateway>
    <interface>ISP</interface>
    <default>1</default>
    <name>ISP</name></command></request>
  </setup>
  <body>
    <packet>
      <request><command name="ip route"></command>
        <assert type="tag" name="error" exist="false" />
        <assert type="tag" name="config" />
      </request>
    </packet>
  </body>
</test>
