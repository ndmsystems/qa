<?xml version="1.0"?>
<test name="ip route. Blank gateway. Negative" groups="mobile">
  <body>
    <packet>
      <request>
      <command name="ip route">
        <default>1</default>
        <gateway></gateway>
        <no>no</no>
        <name></name>
        <mask></mask>
        <interface>ISP</interface>
      </command>
      <assert type="xpath" path="//error[@source='gateway']" value="invalid argument" />
      </request>
    </packet>
  </body>
</test>
