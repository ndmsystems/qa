<?xml version="1.0"?>
<test name="interface ip global. Without description. Negative" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>PPPoE2</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface description">
        <description>description</description>
      </command>
        <assert type="tag" name="error" />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
      <command name="interface">
        <name>PPPoE2</name>
        <no>no</no>
      </command>
    </request>
  </teardown>
</test>
