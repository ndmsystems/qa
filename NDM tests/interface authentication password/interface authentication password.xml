<?xml version="1.0"?>
<test name="interface authentication password. Full. Positive" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>PPPoE2</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface authentication password">
        <name>PPPoE2</name>
        <password>password</password>
      </command>
        <assert type="xpath" path="//message[text()='done']"/>
        <assert type="tag" name="error" exists="false" />
        <assert type="xpath" path="//message[text()='Identity saved.']" />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
    <command name="interface">
      <name>PPPoE2</name>
      <no>no</no>
    </command>
  </request>
  </teardown>
</test>
