<?xml version="1.0"?>
<test name="ip name-server.No without address. Negative" groups="mobile">
  <setup>
    <request>
      <command name="ip name-server">
        <address>192.168.1.33</address>
        <domain>some</domain>
        <interface>ISP</interface>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="ip name-server">
        <domain>some</domain>
        <interface>ISP</interface>
        <no>no</no>
        </command>
        <assert type="error" />
      </request>
    </packet>
  </body>
</test>
