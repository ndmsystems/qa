<?xml version="1.0"?>
<test name="interface ip tcp adjust-mss. Without string and phone. Negative" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>PPPoE4</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface ip tcp adjust-mss">
        <name>UsbModem2</name>
      </command>
        <assert type="tag" name="error" />
        <assert type="tag" name="message" />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
      <command name="interface">
        <name>PPPoE4</name>
        <no>no</no>
      </command>
    </request>
  </teardown>
</test>

