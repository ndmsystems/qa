<?xml version="1.0"?>
<test name="interface ip tcp adjust-mss. Without name. Negative" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>PPPoE4</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface ip tcp adjust-mss">
        <mss type="Integer">2</mss>
        <pmtu>1</pmtu>
      </command>
        <assert type="error" />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
      <command name="interface">
        <name>PPPoE4</name>
        <no>no</no>
      </command>
    </request>
  </teardown>
</test>
