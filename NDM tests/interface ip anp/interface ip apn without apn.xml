<?xml version="1.0"?>
<test name="interface ip apn. No apn. Positive" groups="mobile">
  <setup>
    <request>
      <command name="interface">
        <name>UsbModem2</name>
      </command>
    </request>
  </setup>
  <body>
    <packet>
      <request><command name="interface ip apn">
        <name>UsbModem2</name>
      </command>
        <assert type="tag" name="error" exist="true" />
      </request>
    </packet>
  </body>
  <teardown>
    <request>
      <command name="interface">
        <name>UsbModem2</name>
        <no>no</no>
      </command>
    </request>
  </teardown>
</test>
